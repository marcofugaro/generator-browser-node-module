{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "keywords": [
    <%- keywords.map(word => JSON.stringify(word)).join(',\n    ') %>
  ],
  "scripts": {
    "build": "rollup -c && npm run minify",
    "minify": "minify build/<%= repoName %>.js --out-file build/<%= repoName %>.min.js",
    "pretest": "npm run build",
    "test": "ava",
    "prepublishOnly": "npm run test"
  },
  "main": "build/<%= repoName %>.js",<% if (moduleField) { %>
  "module": "build/<%= repoName %>.esm.js",<% } %>
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=6"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-minify": "^0.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "browser-env": "^3.2.4",
    "rollup": "^0.52.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
