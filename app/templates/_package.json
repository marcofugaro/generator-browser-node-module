{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "keywords": [
    <%- keywords.map(word => JSON.stringify(word)).join(',\n    ') %>
  ],
  "scripts": {
    "build": "rollup -c && npm run minify",
    "minify": "terser build/<%= repoName %>.js --output build/<%= repoName %>.min.js",
    "pretest": "npm run build",
    "test": "ava",
    "prepublishOnly": "npm run test"
  },
  "main": "build/<%= repoName %>.js",<% if (moduleField) { %>
  "module": "build/<%= repoName %>.esm.js",<% } %>
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=6"
  },
  "ava": {
    "require": [
      "@babel/register",
      "core-js/stable",
      "regenerator-runtime/runtime",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "ava": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "browser-env": "^3.2.6",
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.1.3"
  }
}
